	  REFERENCES "CTR_TIPO_PERSONAL" ("ID_TIPO_PERSONAL") ENABLE;

CREATE OR REPLACE EDITIONABLE TRIGGER "BI_CTR_USUARIOS" 
  BEFORE INSERT ON CTR_USUARIOS               
  FOR EACH ROW  
BEGIN   
  IF :NEW.ID_USUARIO IS NULL THEN 
    SELECT CTR_USUARIOS_SEQ.NEXTVAL INTO :NEW.ID_USUARIO FROM SYS.DUAL; 
  END IF; 
END;

/


ALTER TRIGGER "BI_CTR_USUARIOS" ENABLE;

