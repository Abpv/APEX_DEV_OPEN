   ) ;

CREATE TABLE "CTR_TOTAL_FACTURACION" 
   (	"ID_TOTAL_FACT" NUMBER, 
	"CREATED_BY" VARCHAR2(20), 
	"CREATED_DATE" TIMESTAMP (6), 
	"TOTAL_FIJO_MENSUAL" NUMBER, 
	"TOTAL_HORA" NUMBER, 
	"TOTAL_EVENTO" NUMBER, 
	"TOTAL_VENTA" NUMBER, 
	"TOTAL_VARIABLE" NUMBER, 
	"TOTAL_PENALIZACIONES" NUMBER, 
	"ID_FICHA" VARCHAR2(80), 
	"ID_SERVICIO" NUMBER, 
	"MONTH" NUMBER, 
	"YEAR" NUMBER, 
	"FASE" VARCHAR2(10), 
	"TOTAL_FACTURACION_FASE" NUMBER, 
	 CONSTRAINT "CTR_TOTAL_FACTURACION_PK" PRIMARY KEY ("ID_TOTAL_FACT")
  USING INDEX  ENABLE
   ) ;

CREATE TABLE "CTR_OPERACION" 
   (	"ID_OPERACION" NUMBER(10,0), 
	"CREATED_DATE" TIMESTAMP (6), 
	"CREATED_BY" VARCHAR2(255), 
	"ID_FICHA" VARCHAR2(80), 
	"TIPO_OPERACION" NUMBER, 
	"SUB_TIPO_OPERACION" NUMBER, 
	"SUB_TIPO_LLAMADA" VARCHAR2(40), 
	"VOLUMEN" NUMBER(10,2), 
	"AHT" NUMBER(5,2), 
	"YEAR" NUMBER(4,0), 
	"MONTH" NUMBER(2,0), 
	"TIPO_DETALLE" NUMBER, 
	"ID_SERVICIO" NUMBER, 
	"FASE" VARCHAR2(10), 
	 CONSTRAINT "CTR_OPERACION_PK" PRIMARY KEY ("ID_FICHA", "SUB_TIPO_OPERACION", "TIPO_OPERACION")
  USING INDEX  ENABLE
   ) ;

ALTER TABLE "CTR_COSTE_PERSONAL_GLOBAL" ADD CONSTRAINT "CTR_COSTE_PERSONAL_GLOBAL_FK" FOREIGN KEY ("ID_FICHA")
	  REFERENCES "CTR_FICHA" ("AI_ID") ENABLE;

ALTER TABLE "CTR_AUXILIAR" ADD CONSTRAINT "CTR_AUXILIAR_FK" FOREIGN KEY ("ID_FICHA")
	  REFERENCES "CTR_FICHA" ("AI_ID") ENABLE;

ALTER TABLE "CTR_SUBTIPO_FACTURACION" ADD CONSTRAINT "CTR_SUBTIPO_FACTURACION_FK" FOREIGN KEY ("TIPO_FACTURACION")
